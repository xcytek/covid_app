<?xml version='1.0' encoding='utf-8'?>
<widget id="io.alexacosta.covid19" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Covid 19 Analisis</name>
    <description>
        Determina si un paciente es caso sospechoso de COVID19 en base a preguntas.
    </description>
    <author email="alex.acosta.me@gmail.com" href="http://xcytek.com">
        Alex Acosta
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
